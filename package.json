{
  "name": "@dillionmegida/react-tour",
  "version": "1.0.3",
  "description": "A customizable React component for creating guided tours in your applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "NODE_ENV=development jest",
    "test:watch": "NODE_ENV=development jest --watchAll",
    "test:coverage": "NODE_ENV=development jest --coverage",
    "prepare": "husky install",
    "prettier": "prettier --write src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "vite build",
    "dev": "vite",
    "size": "npm run build && size-limit",
    "prepublishOnly": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,css,scss,json,jsx,ts,tsx}": "prettier --write"
  },
  "keywords": [],
  "author": "Dillion Megida",
  "license": "ISC",
  "devDependencies": {
    "@size-limit/preset-big-lib": "^11.2.0",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "lint-staged": "^15.5.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-embedded": "^1.71.1",
    "size-limit": "^11.2.0",
    "storybook": "^7.6.17",
    "terser": "^5.29.2",
    "test-node": "^0.0.2",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
